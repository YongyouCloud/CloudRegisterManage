<wpf:Dialog
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:b="http://www.scheshan.com/bootstrap"
        x:Class="WPF.Bootstrap.Demo.MainWindow"
        xmlns:wpf="http://github.com/scheshan/bootstrap"
        xmlns:view="clr-namespace:WPF.Bootstrap.Demo.Views"
        xmlns:form="clr-namespace:CloudRegisterManage.Views"
        Title="云注册管理" Height="800" Width="1000">

    <TabControl TabStripPlacement="Left" >
        <TabItem Header="注册产品">
            <form:ProductReg/>
        </TabItem>
        <TabItem Header="导入许可" Height="37" VerticalAlignment="Bottom">
            <form:ImportLic/>
        </TabItem>
        <TabItem Header="分配许可">
            <form:AssignLic/>
        </TabItem>
        <TabItem Header="备份许可">
            <form:BackupLic/>
        </TabItem>
        <TabItem Header="查看特征码">
            <form:HardCodeForm/>
        </TabItem>
        <TabItem Header="查看许可占用情况">
            <form:LicUseInfo/>
        </TabItem>
    </TabControl>
</wpf:Dialog>
